<h1>USER_NAME's stash</h1>

<h3>My Artists</h3>
<% @artists.each do |artist| %>
	<li><%= artist.name %> - playing at X festivals I'm going to, X festivals I'm watching</li>
<% end %>


<h3>My Festivals</h3>
<h4>I'm Going</h4>
<% @festivals_going.each do |festival| %>
	<li><%= festival.name %></li>
<% end %>

<h4>I'm Watching</h4>
<% @festivals_watching.each do |festival| %>
	<li><%= festival.name %></li>
<% end %>