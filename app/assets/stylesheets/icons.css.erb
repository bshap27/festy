@font-face {
	font-family: 'icomoon';
	src:url(<%= asset_path 'icomoon.eot?-pco6za' %>);
	src:url(<%= asset_path 'icomoon.eot?#iefix-pco6za' %>) format('embedded-opentype'),
		url(<%= asset_path 'icomoon.woff?-pco6za' %>) format('woff'),
		url(<%= asset_path 'icomoon.ttf?-pco6za' %>) format('truetype'),
		url(<%= asset_path 'icomoon.svg?-pco6za#icomoon' %>) format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

